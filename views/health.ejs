<!DOCTYPE html>
<html>
<head>
  <% include partials/template/head.ejs %>
</head>

<body class="<%= page %>">

<div class="container">
  <div class="outline">
    <% include partials/template/header.ejs %>

    <% include partials/template/headEnding.ejs %>

    <section class="layout">
      <div class="primary">

        <div id="fb-root"></div>

        <div class="welcome">




          <div class="healthInsurance">
            <div class="insuranceTypeHeading">Health Insurance</div>


            <div class="insuranceLeftList">
              <ul class="ui-insuranceLeftList">
                <li>Health Policy Number: <span><%= memberData.HealthI.plan_id %></span></li>
                <li>Health Policy Start Date: <span><%= memberData.HealthI.created_date %></span></li>
                <li>Health Policy Renewal Date: <span><%= memberData.HealthI.renewal_date %></span></li>
                <li>Health Policy Duration: <span><%= memberData.HealthI.period_in_months %> years </span> </li>
              </ul>

              <div class="benefits">
                <h3><span>Benefits</span></h3>
                <p>
                  <span class="benefits-heading">
                    <%=memberData.HealthI.eligibility[0].glasses %>
                  </span><br>
                  <%=memberData.HealthI.eligibility[0].glasses_description %>
                </p>

                <p>
                  <span class="benefits-heading">
                    <%=memberData.HealthI.eligibility[0].drugs %>
                  </span><br>
                  <%=memberData.HealthI.eligibility[0].drugs_description %>
                </p>

                <p>
                  <span class="benefits-heading">
                    <%=memberData.HealthI.eligibility[0].dental %>
                  </span><br>
                  <%=memberData.HealthI.eligibility[0].dental_description %>
                </p>

                <p>
                  <span class="benefits-heading">
                    <%=memberData.HealthI.eligibility[0].screening %>
                  </span><br>
                  <%=memberData.HealthI.eligibility[0].screening_description %>
                </p>

              </div>


            </div>


            <ul class="insuranceRightList">
              <li><a href="#">How to make a claim</a></li>
              <li><a href="#">Add other benefits</a></li>
              <li><a href="#">View policy document</a></li>
            </ul>
          </div>

          <hr>

          <div class="userInsurance">

            <div class="ui-userInfo">
              <div class="userTypeHeading">USER ACCOUNT</div>

              <ul>
                <li>Account Reference: <span><%= memberData.payer_id %></span></li>
                <li>Full Name: <span><%= memberData.payer_name %></span></li>
                <li>Date Of Birth: <span><%= memberData.dob %></span></li>
                <li>Gender: <span><%= memberData.gender %></span></li>
                <li>Email: <span><%= memberData.registered_email %></span></li>
                <li>Mobile Number: <span><%= memberData.registered_mobile %></span></li>
                <li class="address">Address: <span>
                    <%= memberData.address.street_line_1 %><br>
                    <%= memberData.address.street_line_2 %><br>
                    <%= memberData.address.zip %>,<br>
                    <%= memberData.address.country %>
                  </span></li>
                <li class="edit_account_details"><a href="#">Edit User Account</a></li>

              </ul>

            </div>
          </div>


        </div>




      </div><!-- primary section -->

      <aside class="secondary">

      </aside><!-- secondary -->

    </section><!-- layout section -->


  </div>
</div>

<% include partials/template/footer.ejs %>
<% include partials/template/analytics.ejs %>

</body>
</html>